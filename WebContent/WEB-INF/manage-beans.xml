<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>authentication</managed-bean-name>
		<managed-bean-class>fr.jweb.app.mbeans.AuthenticationMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>newsletterSubscriber</managed-bean-name>
		<managed-bean-class>fr.jweb.app.mbeans.NewsletterSubscriberMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>newsList</managed-bean-name>
		<managed-bean-class>fr.jweb.app.mbeans.NewsListMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>productList</managed-bean-name>
		<managed-bean-class>fr.jweb.app.mbeans.ProductListMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reviewList</managed-bean-name>
		<managed-bean-class>fr.jweb.app.mbeans.ReviewListMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userList</managed-bean-name>
		<managed-bean-class>fr.jweb.app.mbeans.UserListMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>